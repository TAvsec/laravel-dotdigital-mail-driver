{
    "name": "tavsec/laravel-dotdigital-mail-driver",
    "description": "Laravel email driver for DotDigital transactional emails",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tavsec\\LaravelDotdigitalMailDriver\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Timotej Avsec",
            "email": "avsectimotej1@gmail.com"
        }
    ],
    "require": {
        "php": ">7.4",
        "illuminate/support": "^9.17",
        "illuminate/mail": "^9.17",
        "guzzlehttp/guzzle": "^7.4",
        "ext-curl": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tavsec\\LaravelDotdigitalMailDriver\\DotdigitalTransportServiceProvider"
            ]
        }
    }
}
